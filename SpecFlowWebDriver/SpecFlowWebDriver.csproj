<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>

    <IsPackable>false</IsPackable>

    <ApplicationIcon />

    <OutputType>Exe</OutputType>

    <StartupObject />

    <LangVersion>preview</LangVersion>
</PropertyGroup>

  <ItemGroup>
    <Compile Remove="Report\**" />
